{
  "name": "ordinals-indexed",
  "version": "0.0.1",
  "description": "Ordinals server starter project with TypeScript and Docker.",
  "main": "index.js",
  "scripts": {
    "ts:watch": "yarn ts --watch",
    "ts": "tsc --noEmit --incremental",
    "dev": "concurrently \"yarn watch-compile\" \"yarn watch-dev\" \"yarn ts:watch\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "prisma-gen": "prisma db pull && prisma generate"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [],
  "author": "topether21",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "4.11.0",
    "axios": "^1.3.4",
    "convert-hrtime": "^5.0.0",
    "cors": "^2.8.5",
    "env-var": "^7.3.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-list-endpoints": "^6.0.0",
    "lodash": "^4.17.21",
    "prisma": "^4.11.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.41",
    "@swc/helpers": "^0.4.14",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "nodemon": "^2.0.21",
    "regenerator-runtime": "^0.13.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  }
}
